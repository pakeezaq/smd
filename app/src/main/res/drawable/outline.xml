<!-- drawable/outline.xml -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <solid android:color="@android:color/transparent" /> <!-- Transparent background -->
    <stroke
        android:width="2dp"
    android:color="@android:color/white" /> <!-- Color of the outline -->
    <corners android:radius="8dp" /> <!-- Adjust the radius of the corners as needed -->
</shape>
